    "ts-node": "^8.0.2",
    "ts-unused-exports": "^2.0.11",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.1"
  },
  "resolutions": {
    "**/ts-jest": "23.1.3",
    "**/jest": "23.6.0"
