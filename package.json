  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "node hooks/prepare-commit-msg.js"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "localStorage": "^1.0.4",
    "lodash": "^4.17.11",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "typesafe-actions": "^2.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.16",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.21",
    "@types/prop-types": "^15.5.8",
