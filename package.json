{
  "name": "react-todomvc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-app-rewired eject",
    "check-updates": "npm-check -u",
    "lint": "eslint ./src/**/*",
    "tslint": "tslint --project . --format verbose",
    "unused": "ts-unused-exports ./tsconfig.json $(find ./src -name '*.ts|x*')"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "node hooks/prepare-commit-msg.js"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run tslint",
      "yarn run lint"
    ],
    "*.js": "yarn run lint"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.0.0-next.3e165448",
    "redux": "^4.0.0-rc.1",
    "reselect": "^4.0.0",
    "typesafe-actions": "^2.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.5.2",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.4",
    "@types/reselect": "^2.2.0",
    "@types/uuid": "^3.4.3",
    "babel-eslint": "^8.2.6",
    "chalk": "^2.4.1",
    "emoji-random": "^0.1.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^3.0.0-next.3e165448",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-types": "^1.0.2",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^1.0.0-rc.12",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.2",
    "prettier": "^1.13.5",
    "react-app-rewire-awesome-typescript": "^0.0.4",
    "react-app-rewired": "^2.0.0",
    "react-scripts-ts": "^2.16.0",
    "ts-node": "^7.0.1",
    "ts-unused-exports": "^2.0.11",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^17.0.1"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
