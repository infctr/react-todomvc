{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "object-literal-sort-keys": [true, "ignore-case", "shorthand-first"],
    "ordered-imports": false
  },
  "jsRules": {
    "object-literal-sort-keys": [true, "ignore-case", "shorthand-first"],
    "ordered-imports": false
  }
}
